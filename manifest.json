{
	"manifest_version": 2,
	"name": "my posting",
	"version": "1.0",
	"icons": {
		"128": "icon.png"
	},
	"content_scripts": [
		{
			"matches": [
				"*://*/*"
			],
			"js": [
				"contentScript.js"
			]
		}
	],
	"background": {
		"scripts": []
	},
	"browser_action": {
		"default_title": "MY",
		"default_icon": "icon.png",
		"default_popup": "index.html"
	},
	"web_accessible_resources": [],
	"permissions": [
		"https://www.asos.com/*",
		"tabs"
	]
}
